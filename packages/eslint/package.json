  },
  "peerDependencies": {
    "@eslint/js": "~9.39.2",
    "eslint": "~9.39.2",
    "eslint-config-prettier": "~10.1.8",
    "typescript-eslint": "~8.53.1"
  },
  "devDependencies": {
    "@types/node": "~22.0.0",
    "typescript": "~5.9.3"
